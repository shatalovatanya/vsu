import time

start = time.clock()
i = 1 + 1
past = (time.clock() - start)
print(past)

start = time.time()
k = 23456789*6758757987654
past = (time.time() - start)
print(past, "время работы без задержки")

start = time.time()
k = 23456789*6758757987654
time.sleep(5)
past = (time.time() - start)
print(past, "с задержкой")
